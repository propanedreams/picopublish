    <script>
        function playVideo() {
            var video = document.getElementById('myVideo');
            video.style.display = 'block';
            video.play();
        }

        // Random functions to add noise
        function initiateRandomProcess() {
            var x = 0;
            for (var i = 0; i < 100; i++) {
                x += Math.sin(i) * Math.random();
            }
            console.log('Random process initiated: ' + x);
        }

        function startUselessOperation() {
            var y = '';
            for (var j = 0; j < 10000; j++) {
                y += String.fromCharCode((Math.random() * 25) + 65);
            }
            console.log('Useless operation completed: ' + y.substr(0, 10));
        }

        function extraLayerOfConfusion() {
            var divs = document.querySelectorAll('.hiddenDiv');
            for (var k = 0; k < divs.length; k++) {
                divs[k].style.display = (k % 2 === 0) ? 'block' : 'none';
            }
            setTimeout(extraLayerOfConfusion, 5000);
        }
        extraLayerOfConfusion();
    </script>
